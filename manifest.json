{
    "extension_id": "formiflex",
    "name": "Formiflex",
    "version": "2.7.1",
    "description": "Lightweight Gutenberg contact form builder, blazingly fast with no external dependencies and ReCaptcha support.",
    "author": "Jankx Team",
    "license": "GPL-2.0+",
    "requires": "6.6",
    "tested": "6.8",
    "requires_php": "7.0",
    "dependencies": {
        "jankx": "2.0.0"
    },
    "caller": {
        "class": "Formiflex\\Extension\\FormiflexExtension",
        "file": "includes/Extension/FormiflexExtension.php",
        "method": "register_hooks",
        "args": [],
        "autoload": true,
        "priority": 10
    },
    "assets": {
        "admin": {
            "css": ["build/style-admin.css"],
            "js": ["build/admin.js", "build/form-settings.js", "build/field-variations.js"]
        },
        "frontend": {
            "css": [],
            "js": []
        }
    },
    "hooks": {
        "init": ["register_blocks"],
        "wp_enqueue_scripts": ["enqueue_frontend_assets"],
        "enqueue_block_editor_assets": ["enqueue_editor_assets"],
        "admin_enqueue_scripts": ["enqueue_admin_assets"]
    },
    "textdomain": "formiflex",
    "domain_path": "/languages"
}
